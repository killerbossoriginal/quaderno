		{"id":"316e8f4431adcadd","type":"text","text":"Deve essere usata con lo spirito critico","x":460,"y":-176,"width":250,"height":60},
		{"id":"4b7ddbc23c00905c","type":"text","text":"Ottimismo sulla natura umana","x":252,"y":-50,"width":250,"height":60},
		{"id":"be2f5cf72a9a627b","type":"text","text":"Si oppone all'assolutismo","x":-540,"y":-176,"width":250,"height":60},
